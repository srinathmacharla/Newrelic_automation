          "rawConfiguration": {
            "nrqlQueries": [
              {
                "accountIds": [
                  4339676
                ],
                "query": "SELECT average('aws.lambda.Duration.byFunction') / 1000 FROM Metric WHERE tags.Environment='preprod' AND tags.Workspace='default' SINCE 1 hour ago TIMESERIES AUTO FACET tags.Application,entityName"
